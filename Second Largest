Problem Code:FLOW017


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int a = s.nextInt();
		    int b = s.nextInt();
		    int c = s.nextInt();
		    System.out.println(a+b+c - Math.max(a,Math.max(b,c))  
		                             - Math.min(a,Math.min(b,c)));
		}
	}
}

