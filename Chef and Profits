Problem Code:CHFPROFIT


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int x = s.nextInt();
		    int y = s.nextInt();
		    int z = s.nextInt();
		    int buy = x*y;
		    int sell = x*z;
		    System.out.println(sell - buy);
		}
	}
}

