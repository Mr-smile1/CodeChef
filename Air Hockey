Problem Code:AIRHOCKEY


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int a = s.nextInt();
		    int b = s.nextInt();
		    int max = Math.max(a,b);
		    System.out.println(7 - max);
		}
	}
}

