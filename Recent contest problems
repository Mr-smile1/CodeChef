Problem Code:RECENTCONT


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		while(t-- > 0){
		    int c=0;
	        int l=0;
	        int n=sc.nextInt();
	        for(int j=0;j<n;j++){
	            String s=sc.next();
	            if(s.charAt(0)=='S')c++;
	            else if(s.charAt(0)=='L')l++;
	        }
	        System.out.println(c+" "+l);
		    
		}
	}
}


