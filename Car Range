Problem Code:CARRANGE


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int p = s.nextInt();
		    int m = s.nextInt();
		    int v = s.nextInt();

		    System.out.println((m-p+1)*v);
		}
	}
}

