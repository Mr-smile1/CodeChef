Problem Code:PRIZEPOOL


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int x = s.nextInt();
		    int y = s.nextInt();
		    int prizex = x*10;
		    int prizey = y*90;
		    System.out.println(prizex + prizey);
		}
	}
}

