Problem Code:SOLBLTY


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int x = s.nextInt();
		    int a = s.nextInt();
		    int b = s.nextInt();

		    System.out.println((((100 - x) *b)+ a )* 10);
		}
	}
}


