Problem Code:BINTREE


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		while(n-- > 0){
		    int i = s.nextInt();
		    int j = s.nextInt();
		    
		    int count = 0;
		    
		    while(i!=j){
		        if(i>j){
		            i/=2;
		        }
		        else{
		            j/=2;
		        }
		        count++;
		    }
		    System.out.println(count);
		}
	}
}


