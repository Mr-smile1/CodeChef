Problem Code:TWOROOKS


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int r1 = s.nextInt();
		    int c1 = s.nextInt();
		    int r2 = s.nextInt();
		    int c2 = s.nextInt();
		    
		    if(r1 == r2 || c1 == c2){
		        System.out.println("YES");
		    }
		    else{
    		    System.out.println("NO");
		    }
		}
	}
}

