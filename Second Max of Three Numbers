Problem Code:SNDMAX


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int x = s.nextInt();
		    int y = s.nextInt();
		    int z = s.nextInt();
		    System.out.println((x+y+z) - Math.max(x,Math.max(y,z)) - Math.min(x,Math.min(y,z)));
		}
	}
}

