Problem Code:CHFCLASS


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int n = s.nextInt();
            if(n%7==6){
                System.out.println((n/7)+1);
            }
            else{
		        System.out.println(n/7);
            }
		}
	}
}


