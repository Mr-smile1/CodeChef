Problem Code:SONGS


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int n = s.nextInt();    // journey 
		    int x = s.nextInt();    // time
		    System.out.println(n/(x*3));    //6%3
		}
	}
}

