Problem Code: M1ENROL


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int x = s.nextInt();
		    int y = s.nextInt();
		    
		    if(x > y){
		        System.out.println("0");
		    }
		    else{
		        System.out.println(y-x);
		    }
		}
	}
}

