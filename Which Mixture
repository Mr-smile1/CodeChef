Problem Code:MIXTURE


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int a = s.nextInt();
		    int b = s.nextInt();

		    if(a > 0 && b > 0){
		        System.out.println("Solution");
		    }
            else if(a == 0){
		        System.out.println("Liquid");
		    }
		    else{
		        System.out.println("Solid");
		    }
		}
	}
}


