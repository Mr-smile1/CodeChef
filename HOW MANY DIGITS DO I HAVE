Problem Code:HOWMANY


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		
		int count = 0;
        while(n > 0){
            n = n/10;
            count++;
        }
        
        if(count >= 4){
            System.out.println("More than 3 digits");
        }
        else{
            System.out.println(count);
        }
	}
}


