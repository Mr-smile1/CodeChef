Problem Code:AREAPERI


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		    int l = s.nextInt();
		    int b = s.nextInt();
		    int area = l*b;
		    int peri = 2*(l+b);
		    if(area > peri){
		        System.out.println("Area");
		        System.out.println(area);
		    }
		    else if(area < peri){
		        System.out.println("Peri");
		        System.out.println(peri);
		    }
		    else{
		        System.out.println("Eq");
		        System.out.println(peri);
		    }
	}
}

