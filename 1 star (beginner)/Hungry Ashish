Problem Code:PIZZA_BURGER


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int x = s.nextInt();
		    int y = s.nextInt();
		    int z = s.nextInt();

		    if(x >= y){
		        System.out.println("PIZZA");
		    }
		    else if(x >= z){
		        System.out.println("BURGER");
		    }
		    else{
		        System.out.println("NOTHING");
		    }
		}
	}
}

