Problem Code:CHFDBT


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int n = s.nextInt();

		    if(n%2==0){
		        System.out.println(n/2);
		    }
		    else{
		        System.out.println((n/2)+1);
		    }
		}
	}
}


