Problem Code:UWCOI20A


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int n = s.nextInt();
		    
		    int max = 0;
		    for(int i=0;i<n;i++){
		        int v = s.nextInt();
		        max = Math.max(max, v);
		    }
		    System.out.println(max);
		}
	}
}

