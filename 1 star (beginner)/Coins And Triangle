Problem Code:TRICOIN


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int n = s.nextInt();
		    
		    int i=1;
		    while(n >= 0){
		        n-=i;
		        i++;
		    }
		    System.out.println(i-2);
		}
	}
}

