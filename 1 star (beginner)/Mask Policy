Problem Code:MASKPOL


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int n = s.nextInt();
		    int a = s.nextInt();
		    int sum = n-a;
		    int min = Math.min(a, sum);

		    System.out.println(min);
		}
	}
}

