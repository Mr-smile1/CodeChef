Problem Code:CHEFSTEP


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int n = s.nextInt();
		    int k = s.nextInt();
		    
		    String st="";
		    
		    for(int i=0;i<n;i++){
		        int val=s.nextInt();
		        if(val%k==0)
		        st+='1';
		        else
		        st+='0';
		        
		    }
		    System.out.println(st);
		}
	}
}


