Problem Code:POOK


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int n = s.nextInt();
		    
		    if(n <= 2){
		        System.out.println(1);
		    }
		    else if(n == 3){
		        System.out.println(n-1);
		    }
		    else{
		        System.out.println(n);
		    }
		}
	}
}


