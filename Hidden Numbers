Problem Code:UWCOI21A


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int n = s.nextInt();
		    
		    if(n%10 == 0){
		        System.out.println(2 + " " + n/2);
		    }
		    else{
		        System.out.println(1 + " " + n);
		    }
		}
	}
}


