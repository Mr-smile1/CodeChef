Problem Code:SNAPE


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int x = s.nextInt();
		    int y = s.nextInt();
		    float h1 = (float)Math.sqrt((y*y) - (x*x));
		    float h2 = (float)Math.sqrt((y*y) + (x*x));
		    System.out.println(h1 + " " + h2);
		}
	}
}


