Problem Code:ACCURACY


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int x = s.nextInt();
		    int rem = x%3;
		    if(rem == 0){
		        System.out.println(0);
		    }
		    else{     
		        System.out.println(3-rem);
		    }
		}
	}
}


