Problem Code:TWOVSTEN


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int x = s.nextInt();

		    if(x%10 == 0){
		        System.out.println("0");
		    }
		    else if(x%5 == 0){
		        System.out.println("1");
		    }
		    else{
		        System.out.println("-1");
		    }
		}
	}
}

