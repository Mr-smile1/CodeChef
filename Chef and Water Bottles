Problem Code:CHEFBOTTLE


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int n = s.nextInt();
		    int x = s.nextInt();
		    int k = s.nextInt();
		    if(x > k){
		        System.out.println(0);
		    }
		    else if(k / x <= n){
		        System.out.println(k/x);
		    }
		    else{
		        System.out.println(n);
		    }
		}
	}
}


