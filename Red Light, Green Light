Problem Code:DOLL


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int n = s.nextInt();
		    int k = s.nextInt();
		    
		    int count=0;
            for(int i=0;i<n;i++){
                int h = s.nextInt();
                if(h > k){
                    count++;
                }
            }
            

		    System.out.println(count);
		}
	}
}

