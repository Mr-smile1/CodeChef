Problem Code:MY1STCONTEST


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		int a = s.nextInt();
		int b = s.nextInt();
		System.out.print(n-a);
		System.out.print(" ");
		System.out.print(n-a-b);
	}
}

