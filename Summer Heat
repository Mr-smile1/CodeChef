Problem Code:COCONUT


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int a = s.nextInt();
		    int b = s.nextInt();
		    int x = s.nextInt();
		    int y = s.nextInt();

		    System.out.println(x/a + y/b);
		}
	}
}

