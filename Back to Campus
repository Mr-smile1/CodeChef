Problem Code:MINDAYSRET


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int n = s.nextInt();
		    int k = s.nextInt();
		    
		    if(n%k == 0){
		        System.out.println(n/k);
		    }
		    else {
		        System.out.println((n/k) + 1);
		    }
		}
	}
}

