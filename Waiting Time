Problem Code:WAITTIME


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int k = s.nextInt();
		    int x = s.nextInt();
		    int daysk = k*7;
		    System.out.println(daysk - x);
		}
	}
}

