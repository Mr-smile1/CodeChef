Problem Code:FLOW007


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int i = s.nextInt();
		    int rev = 0;
		    while(i != 0) {
                int digit = i % 10;
                rev = rev * 10 + digit;
                i /= 10;
            }
            System.out.println(rev);
		}
    } 
}

