Problem Code:JDELAY


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int n = s.nextInt();
		    int count=0;

		    while(n-- > 0){
		        int x = s.nextInt();
		        int y = s.nextInt();
		        if((y - x) > 5){
		            count++;
		        }
		    }

		    System.out.println(count);
		}
	}
}


