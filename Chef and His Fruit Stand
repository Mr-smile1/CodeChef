Problem Code:FRUITCHAAT


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int x = s.nextInt();
		    int y = s.nextInt();
		    x /= 2;
		    System.out.println(Math.min(x,y));
		}
	}
}

