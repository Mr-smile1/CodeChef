Problem Code:CGYM


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int x = s.nextInt();
		    int y = s.nextInt();
		    int z = s.nextInt();
		    int entry = y*z;
		    if(x+y <= z){
		        System.out.println(2);
		    }
		    else if(x <= z){
		        System.out.println(1);
		    }
		    else{
		        System.out.println(0);
		    }
		}
	}
}

