Problem Code: AUCTION


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner hh = new Scanner(System.in);
		int t = hh.nextInt();
		while(t-- > 0){
		    int a = hh.nextInt();
		    int b = hh.nextInt();
		    int c = hh.nextInt();
		    if(a > b && a > c){
		        System.out.println("Alice");
		    }
		    else if(b > a && b > c){
		        System.out.println("Bob");
		    }
		    else{
		        System.out.println("Charlie");
		    }
		}
	}
}


