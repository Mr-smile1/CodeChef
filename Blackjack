Problem Code:BLACKJACK


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int a = s.nextInt();
		    int b = s.nextInt();
		    int sum = a+b;
		    if(sum>=11)
		    {
		        System.out.println(21-sum);
		    }
		    else{
		        System.out.println(-1);
		    }
		}
	}
}


