Problem Code:SHOEFIT


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int x = s.nextInt();
		    int y = s.nextInt();
		    int z = s.nextInt();
		    if(x+y+z == 0 || x+y+z == 3){
		        System.out.println("0");
		    }
		    else{
		        System.out.println("1");
		    }
		} 
	}
}


