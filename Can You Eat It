Problem Code:COMPCAND


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int n = s.nextInt();
		    int k = s.nextInt();
		    
		    if(n == 0){
		        System.out.println("0");
		    }
		    else if(n%k==0){
		        System.out.println(n/k);
		    }
		    else{
		        System.out.println("-1");
		    }
		}
	}
}


