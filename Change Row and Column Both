Problem Code: CHANGEPOS


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
    Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int w = s.nextInt();
		    int x = s.nextInt();
		    int y = s.nextInt();
		    int z = s.nextInt();
		    if(w==y || x==z){
		        System.out.println(2);
		    }
		    else{
		        System.out.println(1);
		    }
		}
	}
}

