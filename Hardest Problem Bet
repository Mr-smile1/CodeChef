Problem Code:HARDBET


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-- > 0){
		    int a = s.nextInt();
		    int b = s.nextInt();
		    int c = s.nextInt();
            
		    if(a<b && a<c){
		        System.out.println("draw");
		    }
		    if(b<a && b<c){
		        System.out.println("Bob");
		    }
		    if(c<a && c<b){
		        System.out.println("Alice");
		    }
		}
	}
}


